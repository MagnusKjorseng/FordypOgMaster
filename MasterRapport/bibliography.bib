	pages = {113570},
	file = {PDF:/media/Bigboi/Documents/FordypOgMaster/Literaturstudie/storage/RL7JR8ZU/Viel et al. - 2023 - ROV localization based on umbilical angle measurement.pdf:application/pdf},
}

@article{trslic_vision_2020,
	title = {Vision based autonomous docking for work class {ROVs}},
	volume = {196},
